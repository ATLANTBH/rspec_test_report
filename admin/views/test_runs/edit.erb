<div class="block">
  <div class="secondary-navigation">
    <ul class="wat-cf">
      <li class="first"><%= link_to pat(:list), url(:test_runs, :index) %></li>
      <li><%= link_to pat(:new), url(:test_runs, :new) %></li>
      <li class="active"><%= link_to pat(:edit), url(:test_runs, :edit, :id => @test_run.id) %></li>
    </ul>
  </div>
  <div class="content">
    <h2 class="title"><%= pat(:edit) %> <% mt(:test_run) %></h2>
    <div class="inner">
      <% form_for :test_run, url(:test_runs, :update, :id => @test_run.id), :method => :put, :class => :form do |f| %>
        <%= partial "test_runs/form", :locals => { :f => f } %>
      <% end %>
    </div>
  </div>
</div>
<% content_for :sidebar, partial("base/sidebar") %>
